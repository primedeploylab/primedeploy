{
  "name": "deployprime-portfolio",
  "version": "1.0.0",
  "description": "DeployPrime Portfolio - Full Stack Application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm install && npm run build",
    "start": "cd backend && node server.js",
    "render-build": "cd backend && npm install && cd ../frontend && npm install && npm run build",
    "seed": "cd backend && npm run seed",
    "create-admin": "cd backend && npm run create-admin",
    "test-db": "cd backend && npm run test-db"
  },
  "keywords": ["portfolio", "fullstack", "react", "node"],
  "author": "DeployPrime",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
